{
  "name": "nestjs-aws-utilities",
  "version": "0.1.7",
  "description": "NestJS Package including abstractions for aws",
  "author": "Renan Zelaya <renan_zelaya@hotmail.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/rzdevelop/nestjs-aws-utilities.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' -c .eslintrc.js",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "deploy": "npm version patch && npm publish"
  },
  "keywords": [
    "nestjs",
    "aws",
    "aws-sdk"
  ],
  "license": "MIT",
  "devDependencies": {
    "@nestjs/common": "^8.0.0",
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "aws-sdk": "^2.1062.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "files": [
    "lib/**/*"
  ],
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "aws-sdk": "^2.1062.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  }
}
